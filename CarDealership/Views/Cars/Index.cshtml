  
@using CarDealership.Models;

@if (@Model.Count == 0)
{
  <p>There are no cars! Good or bad business</p>
}

<ul>
  @foreach (Car car in Model)
  {
    <li>Description: @car.Description</li>
    <li>Model: @car.MakeModel</li>
    <li>Price: @car.Price</li>
    <li>Miles: @car.Miles</li>
  }
</ul>
